@{
    ViewData["Title"] = "Home Page";
}

<div class="row text-center">
    <div class="dashboard mt-3">
        <div class="dashboard-item">
            <a class="nav-link text-light" asp-area="" asp-controller="Home" asp-action="Privacy">
            <i class="bi bi-file-earmark-plus"></i>
            <p>New NCR</p>
            </a>
        </div>
        <div class="dashboard-item">
            <i class="bi bi-file-earmark-spreadsheet"></i>
            <p>NCR List</p>
        </div>
        <div class="dashboard-item">
            <i class="bi bi-buildings"></i>
            <p>Suppliers</p>
        </div>
        <div class="dashboard-item">
            <i class="bi bi-person-bounding-box"></i>
            <p>Users</p>
        </div>
        <div class="dashboard-item">
            <i class="bi bi-graph-down"></i>
            <p>KPI Dashboard</p>
        </div>
        <div class="dashboard-item dropdown">
            <i class="bi bi-building-gear"></i>
            <div class="dropdown">
                <a class="btn btn-secondary dropdown-toggle" role="button" id="dropdownMenuButton" data-toggle="dropdown" aria-haspopup="true" aria-expanded="false">
                    Admin
                </a>
                <div class="dropdown-menu" aria-labelledby="dropdownMenuButton">
                    <a class="dropdown-item" asp-area="" asp-controller="" asp-action="">Items</a>
                    <a class="dropdown-item" asp-area="" asp-controller="" asp-action="">Defects</a>
                    <a class="dropdown-item" asp-area="" asp-controller="" asp-action="">Lookup Values</a>
                </div>
            </div>
        </div>
    </div>
</div>
